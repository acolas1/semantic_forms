# should work but trouble cf commit e01e9a6023bf9bd51c4617a5ecd6566527d52436
# @prefix forms: <#> .

# form specifications for login


forms:loginForm
  a form:specification ;
  form:classDomain form:AuthenticationAttempt ;
  rdfs:label "Authentification"@fr ;
  rdfs:label "Authentication"@en ;
  form:showProperties (
form:userid 
form:password
) .

forms:registerForm
  a form:specification ;
  form:classDomain form:RegisterAccount ;
  rdfs:label "Cr√©er un compte"@fr ;
  rdfs:label "Register Account"@en ;
  form:showProperties (
form:userid 
form:password
form:confirmPassword
) .

forms:userid--loginForm
	:fieldAppliesToForm forms:loginForm ;
	:fieldAppliesToForm forms:registerForm ;
	:fieldAppliesToProperty form:userid ;
	:fieldAppliesToProperty form:password ;
	:fieldAppliesToProperty form:confirmPassword ;
        :cardinality :exactlyOne .

